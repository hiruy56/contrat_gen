{
  "builds": [
    {"src": "main.py", "use": "@vercel/python"}
  ],
  "routes": [
    {"src": "/(.*)", "dest": "main.py"}
  ],
  "headers": [
    {
      "source": "/generate-contract",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
